<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% @users.each do |user| %>
    <tbody>
      <tr>

        <td><%= user.name %></td>
        <td><%= link_to "Show",user_path(user.id) %></td>
    </tbody>
  <% end %>
</table>