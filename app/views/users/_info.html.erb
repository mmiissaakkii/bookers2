  <h2>User info</h2>
  <%= image_tag user.get_profile_image(100,100) %>
  <table class="table">
        <tr>
          <th>name</th>
          <th><%= user.name %></th>
        </tr>
          <th>introduction</th>
          <th><%= user.introduction %></th>
  </table>

  <div class="row">
    <%= link_to edit_user_path(user.id), class: "btn btn-outline-secondary btn-block", method: :get do %>
      <i class="fas fa-user-cog"></i>
    <% end %>
  </div>